{
  "__comment__": "This is part of 'npx unimported' tool to discover unused libraries or unreachable code.",
  "entry": [
    "./server.js"
  ],
  "extensions": [
    ".ts",
    ".js"
  ],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/client/**"
  ],
  "ignoreUnresolved": [
    "some-npm-dependency"
  ],
  "ignoreUnimported": [
    "gists/**",
    "public/**",
    "test/**",
    "docker/**",
    "static/**",
    "ecosystem.config.js"
  ],
  "ignoreUnused": [
    "polka"
  ]
}